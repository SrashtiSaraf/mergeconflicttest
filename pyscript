    #Handler for the GET requests
    def do_GET(self):
        if self.path=="/":
            self.path="/home.html"

        if self.path=="/command/":
            self.send_response(200)
            self.send_header('Content-type','text/html')
            self.end_headers()
            # Send the html message
            self.wfile.write("transfer +85000000000")
            return
